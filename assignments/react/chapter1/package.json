    "draft-build-all": "npm run draft-dev && npm run draft-prod",
    "dev": "npm run core-dev && npm run draft-dev",
    "prod": "npm run core-prod && npm run draft-prod",
    "build-all": "npm run dev && npm run prod",
    "test": "echo \"Error: no test specified\" && exit 1",
